    background-image: -webkit-linear-gradient(left, #FFFFFF 6%, #00FF00 25%, #FFFF00 60%, #FF0000 100%);
    background-image: -ms-linear-gradient(left, #FFFFFF 6%, #00FF00 25%, #FFFF00 60%, #FF0000 100%);
    background-image: -webkit-gradient(linear, left top, right top, color-stop(0.06, #FFFFFF), color-stop(0.25, #00FF00), color-stop(0.6, #FFFF00), color-stop(1, #FF0000));
}

.motivationSlider, .moodSlider, .yesterdaysTrainingSlider {
