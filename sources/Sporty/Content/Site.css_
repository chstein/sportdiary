body, html {
    height: 100%;
    width: 100%;
}

body {
    color: #000;
    font-family: Georgia, "Century Schoolbook L", serif;
    font-size: 62.5%; /*font-family: "Tahoma" , "Arial" , "Verdana" ,sans-serif;*/
    margin: 0;
    padding: 0;
}

a {
    color: #455101;
    text-decoration: underline;
}

a:hover, a:active {
    color: #677323;
    text-decoration: underline;
}

#header {
    background: transparent url('images/header-bg.png') repeat-x;
    height: 102px;
    width: 100%;
}

a#logo {
    background: transparent url('images/logo.png') no-repeat right center;
    display: block;
    height: 60px;
    text-indent: -9999em;
    width: 145px;
}

div#user-box {
    position: absolute;
    right: 10px;
    text-align: right;
    top: 10px;
}

div#user-box div.user-avatar { display: inline-block; }

div#user-box div.user-info {
    display: inline-block;
    padding: 0 8px 2px 0;
    vertical-align: bottom;
}

div#user-box div.user-name {
    color: #333;
    font-size: 1.2em;
    font-weight: bold;
    margin-bottom: 2px;
}

div#user-box div.user-actions { }

div#user-box div.user-actions a:link, div#user-box div.user-actions a:visited {
    color: #666;
    text-decoration: none;
}

div#user-box div.user-actions a:hover, div#user-box div.user-actions a:active {
    color: #000;
    text-decoration: underline;
}

ul#toolbar {
    background: transparent url('images/toolbar-bg-normal.png') repeat-x;
    border-bottom: 2px solid #9999FF;
    border-top: 2px solid #9999FF;
    font-size: 1.4em;
    height: 38px;
    line-height: 1.2em;
    list-style-type: none;
    overflow: hidden;
    width: 100%;
}

ul#toolbar li {
    display: inline;
    float: left;
}

ul#toolbar li a {
    color: #fff;
    display: block;
    height: 38px;
    line-height: 37px;
    padding: 0 18px;
    text-decoration: none;
}

ul#toolbar li a:hover, ul#toolbar li a:active { background: transparent url('images/toolbar-bg-hover.png') repeat-x !important; }

ul#toolbar li.current a {
    background-color: #4F4FFF;
    border-left: 1px solid #5555FF;
    border-right: 1px solid #5555FF;
    padding: 0 17px;
}

ul#toolbar li.toolbar-right {
    float: right;
    right: 0;
}

#footer {
    color: #999;
    font-size: .9em;
    line-height: normal;
    margin: 0;
    padding: 10px 0;
    text-align: center;
}

/* TAB MENU   
----------------------------------------------------------*/

ul#menu {
    border-bottom: 1px #5C87B2 solid;
    margin: 0;
    padding: 0 0 2px;
    position: relative;
    text-align: right;
}

ul#menu li {
    display: inline;
    list-style: none;
}

ul#menu li#greeting {
    color: #fff;
    font-weight: bold;
    line-height: 2.8em;
    padding: 10px 20px;
    text-decoration: none;
}

ul#menu li a {
    background-color: #e8eef4;
    color: #034af3;
    font-weight: bold;
    line-height: 2.8em;
    padding: 10px 20px;
    text-decoration: none;
}

ul#menu li a:hover {
    background-color: #fff;
    text-decoration: none;
}

ul#menu li a:active {
    background-color: #a6e2a6;
    text-decoration: none;
}

ul#menu li.selected a {
    background-color: #fff;
    color: #000;
}

