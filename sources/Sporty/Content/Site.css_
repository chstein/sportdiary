    background-image: -o-linear-gradient(left, #FFFFFF 6%, #FF0000 15%, #FFFF00 50%, #00FF00 100%);
    background-image: -moz-linear-gradient(left, #FFFFFF 6%, #FF0000 15%, #FFFF00 50%, #00FF00 100%);
